-
    const ExampleTypes = {
        'ELEMENT': 'ELEMENT',
        'GROUP': 'GROUP',
        'COMPONENT': 'COMPONENT',
        'MODULE': 'MODULE'
    };

    self.examples = {
        'ELEMENT': [],
        'GROUP': [],
        'COMPONENT': [],
        'MODULE': []
    };

mixin u-example(context = {})
    //-
        Block content is required. It will be attached to the provided context and pushed to cooresponding example list.

        Example
        {
            type: ExampleType,
            headline: String | Object
        }

    - self.examples[context.type].push((context.block = random(), context))

    +u-block(context.block)
        block